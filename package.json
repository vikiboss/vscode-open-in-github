{
  "name": "viki-open-in-github",
  "displayName": "Open Repo in GitHub",
  "publisher": "vikiboss",
  "description": "Display a GitHub icon in the status bar that links to the repository",
  "version": "0.2.0",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "author": {
    "name": "Viki",
    "email": "hi@viki.moe",
    "url": "https://viki.moe"
  },
  "repository": {
    "url": "https://github.com/vikiboss/vscode-open-in-github",
    "type": "github"
  },
  "activationEvents": [
    "onStartupFinished",
    "onDidChangeActiveTextEditor",
    "onDidChangeWorkspaceFolders",
    "onDidChangeConfiguration"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openInGitHub",
        "title": "Open in GitHub",
        "icon": "$(github)",
        "shortTitle": "Open this GitHub repo in default Browser"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.60.0",
    "@vmoe/eslint-config": "^0.3.1",
    "@vmoe/prettier-config": "^1.0.0",
    "eslint": "^8.37.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3"
  },
  "license": "MIT"
}
